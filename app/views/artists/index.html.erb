<h1>Artists</h1>
<ul>
  <% @artists.each do |artist| %>
  <li>Artist: <%= link_to artist.name, artist_path(artist) %>
    <br>Song Count: <%= pluralize(artist.song_count, 'song')%>
    <br>Songs:
    <ul>
      <% artist.songs.each do |song| %>
      <li><%= link_to song.title, song_path(song) %></li>
      <% end %>
    </ul>
  </li>
  <% end %>
</ul>
